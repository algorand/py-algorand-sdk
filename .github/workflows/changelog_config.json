{
    "categories": [
        {
            "title": "## New Features",
            "labels": [
                "New Feature"
            ]
        },
        {
            "title": "## Enhancement",
            "labels": [
                "Enhancement"
            ]
        },
        {
            "title": "## Bug Fixes",
            "labels": [
                "Bug-Fix"
            ]
        },
        {
            "title": "## Not Yet Enabled",
            "labels": [
                "Not-Yet-Enabled"
            ]
        }
    ],
    "ignore_labels": [
        "Skip-Release-Notes"
    ],
    "sort": {
        "order": "ASC",
        "on_property": "mergedAt"
    },
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>Other</summary>\n\n${{UNCATEGORIZED}}\n</details>",
    "pr_template": "- ${{TITLE}} by ${{AUTHOR}} in #${{NUMBER}}",
    "empty_template": "- no changes",
    "duplicate_filter": {
        "pattern": "\\[ABC-....\\]",
        "on_property": "title",
        "method": "match"
    }
}
